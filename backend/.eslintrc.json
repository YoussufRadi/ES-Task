{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "env": {
    "jest": true
  },
  "rules": {
    "function-paren-newline": 0,
    "arrow-parens": 0,
    "no-unused-vars": 0,
    "prefer-destructuring": [
      "error",
      {
        "object": false,
        "array": false
      }
    ]
  }
}
